<h1>ADMINS</h1>

{{#if errors.length}}
  {{#each}}
    {{this}}
  {{/each}}
{{else}}
  <div class="centered">
    <button onclick="window.location.href='/admins/create'">Create new!</button>
  </div>

  {{#if admins.length}}
    {{#each admins}}
      <div class="item">
        <a href="/admins/{{id}}">{{Username}}</a>
      </div>
    {{/each}}
  {{/if}}
{{/if}}