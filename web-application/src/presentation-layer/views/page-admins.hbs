<h1>ADMINS</h1>
{{#if admins.length}}
  {{#each admins}}
  <div class="item">
    <a href="/admins/{{id}}">{{Username}}</a>
  </div>
  {{/each}}
  <button onclick="window.location.href='/admins/create'">Create new!</button>
{{else}}
  <div>
    <p>There is no admins atm..</p>
    <button onclick="window.location.href='/admins/create'">Create new!</button>
  </div>
{{/if}}